{

  "tokenEnding": [],
  "tokensExpected": 1,
  "rules": {
    "(": function() {
      self.nesting++;
    },

    ")": function() {
      if(self.nesting) {
        self.nesting--;
      } else {
        self.lexer.fireToken();
      }
    }
  },
  "structure": [ ["DATA"] ]
  "onToken": {
    "token": "DATA",
    "filter": ["/^\s*\(/," ""], // replace
    // "match": "" not needed any token
    // "scope": "" goes to back to root / one up
  }

}

