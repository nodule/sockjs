{
  "onLineStart": {
    "tokensExpected": 1,
    "tokenEnding": '\n'
  },

  "validate": {
    "META": "/^\w+:/",
    "META_DATA": "/.*/"
  },

  // Used by the lexer to validate token combinations.
  "structure": [
    ['META_DATA']
  ],

  "onToken": {
    "token": "META_DATA",
    "filter": "trim" // TODO: also a bug, it should already be trimmed?
  }

}
